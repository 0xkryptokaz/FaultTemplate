        <script>
        const equipmentDamageSelect = document.getElementById('equipment-damage');
        const damageDescriptionDiv = document.getElementById('damage-description');
        const damageTypeSelect = document.getElementById('damage-type');

        equipmentDamageSelect.addEventListener('change', function () {
            if (equipmentDamageSelect.value === 'damage-yes') {
                damageDescriptionDiv.style.display = 'block';
            } else {
                damageDescriptionDiv.style.display = 'none';
            }
        });
    </script>

